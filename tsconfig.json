{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "types": ["@figma/plugin-typings"],
    "typeRoots": ["node_modules/@types", "node_modules/@figma"],
    "moduleResolution": "node",
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["code.ts"]
}
